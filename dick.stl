solid ultra_detailed_phallus
  # Base Plate (flat for grounding the model)
  facet normal 0 0 -1
    outer loop
      vertex -1.0 -1.0 0
      vertex 1.0 -1.0 0
      vertex 1.0 1.0 0
    endloop
  endfacet
  facet normal 0 0 -1
    outer loop
      vertex -1.0 -1.0 0
      vertex 1.0 1.0 0
      vertex -1.0 1.0 0
    endloop
  endfacet

  # Shaft - very finely segmented cylinder with slight taper and curvature
  # Many rings of vertices for smooth curvature
  # Example for one ring, would be repeated dozens of times
  # Ring at Z = 0.1, Radius = 0.65
  facet normal 0 0 1
    outer loop
      vertex 0.65 0 0.1
      vertex 0.618 0.201 0.1
      vertex 0.521 0.382 0.1
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0.65 0 0.1
      vertex 0.521 0.382 0.1
      vertex 0.382 0.521 0.1
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0.65 0 0.1
      vertex 0.382 0.521 0.1
      vertex 0.201 0.618 0.1
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0.65 0 0.1
      vertex 0.201 0.618 0.1
      vertex 0 0.65 0.1
    endloop
  endfacet
  # ... (Thousands more vertices and facets for the shaft's body)

  # Coronal Ridge - very distinct and rounded edge
  # This is a key anatomical feature, rendered with many small facets
  facet normal 0 0 1
    outer loop
      vertex 0.7 0 4.0
      vertex 0.67 0.20 4.0
      vertex 0.60 0.38 4.0
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0.7 0 4.0
      vertex 0.60 0.38 4.0
      vertex 0.49 0.56 4.0
    endloop
  endfacet
  # ... (Hundreds of facets for the full coronal ridge, connecting it smoothly to the shaft and glans)

  # Glans - finely rounded, complex dome with slight curvature
  facet normal 0 0 1
    outer loop
      vertex 0 0 5.5
      vertex 0.6 0 4.2
      vertex 0.57 0.18 4.2
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0 0 5.5
      vertex 0.57 0.18 4.2
      vertex 0.49 0.34 4.2
    endloop
  endfacet
  # ... (Thousands more facets to form the entire glans dome)

  # Urethral Meatus - a well-defined indentation at the tip
  facet normal 0 0 1
    outer loop
      vertex -0.1 0 5.4
      vertex 0.1 0 5.4
      vertex 0 0.1 5.4
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex -0.1 0 5.4
      vertex 0 -0.1 5.4
      vertex 0.1 0 5.4
    endloop
  endfacet
  # ... (More facets to define the internal shape of the meatus)

endsolid