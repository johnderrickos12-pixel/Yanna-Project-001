solid highly_detailed_phallus
  # Shaft base (slightly flared)
  facet normal 0 0 -1
    outer loop
      vertex -0.7 -0.7 0
      vertex 0.7 -0.7 0
      vertex 0.7 0.7 0
    endloop
  endfacet
  facet normal 0 0 -1
    outer loop
      vertex -0.7 -0.7 0
      vertex 0.7 0.7 0
      vertex -0.7 0.7 0
    endloop
  endfacet
  
  # Shaft body (tapered cylinder)
  facet normal 0 0 1
    outer loop
      vertex -0.6 -0.6 0.1
      vertex 0.6 -0.6 0.1
      vertex 0.6 0.6 0.1
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex -0.6 -0.6 0.1
      vertex 0.6 0.6 0.1
      vertex -0.6 0.6 0.1
    endloop
  endfacet
  
  facet normal 0 0 -1
    outer loop
      vertex -0.5 -0.5 4
      vertex -0.5 0.5 4
      vertex 0.5 0.5 4
    endloop
  endfacet
  facet normal 0 0 -1
    outer loop
      vertex -0.5 -0.5 4
      vertex 0.5 0.5 4
      vertex 0.5 -0.5 4
    endloop
  endfacet

  # Connecting sides of shaft (many facets for smoother curve)
  # This is a simplified example, a real smooth cylinder would require many more tiny facets
  # Front face
  facet normal 0 1 0.1
    outer loop
      vertex -0.6 0.6 0.1
      vertex 0.6 0.6 0.1
      vertex 0.5 0.5 4
    endloop
  endfacet
  facet normal 0 1 0.1
    outer loop
      vertex -0.6 0.6 0.1
      vertex 0.5 0.5 4
      vertex -0.5 0.5 4
    endloop
  endfacet

  # Right face
  facet normal 1 0 0.1
    outer loop
      vertex 0.6 -0.6 0.1
      vertex 0.6 0.6 0.1
      vertex 0.5 0.5 4
    endloop
  endfacet
  facet normal 1 0 0.1
    outer loop
      vertex 0.6 -0.6 0.1
      vertex 0.5 0.5 4
      vertex 0.5 -0.5 4
    endloop
  endfacet

  # Back face
  facet normal 0 -1 0.1
    outer loop
      vertex -0.6 -0.6 0.1
      vertex -0.5 -0.5 4
      vertex 0.5 -0.5 4
    endloop
  endfacet
  facet normal 0 -1 0.1
    outer loop
      vertex -0.6 -0.6 0.1
      vertex 0.5 -0.5 4
      vertex 0.6 -0.6 0.1
    endloop
  endfacet

  # Left face
  facet normal -1 0 0.1
    outer loop
      vertex -0.6 -0.6 0.1
      vertex -0.5 -0.5 4
      vertex -0.5 0.5 4
    endloop
  endfacet
  facet normal -1 0 0.1
    outer loop
      vertex -0.6 -0.6 0.1
      vertex -0.5 0.5 4
      vertex -0.6 0.6 0.1
    endloop
  endfacet

  # Glans (more rounded, using multiple facets to approximate a dome)
  # Ring at base of glans
  facet normal 0 0 1
    outer loop
      vertex -0.5 0.5 4
      vertex 0.5 0.5 4
      vertex 0.6 0.6 4.1
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex -0.5 0.5 4
      vertex 0.6 0.6 4.1
      vertex -0.6 0.6 4.1
    endloop
  endfacet
  # ... (many more facets for a smoother glans dome would go here)
  
  # Top of glans dome
  facet normal 0 0 1
    outer loop
      vertex 0 0 5
      vertex 0.6 0 4.2
      vertex 0 0.6 4.2
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0 0 5
      vertex 0 0.6 4.2
      vertex -0.6 0 4.2
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0 0 5
      vertex -0.6 0 4.2
      vertex 0 -0.6 4.2
    endloop
  endfacet
  facet normal 0 0 1
    outer loop
      vertex 0 0 5
      vertex 0 -0.6 4.2
      vertex 0.6 0 4.2
    endloop
  endfacet

endsolid